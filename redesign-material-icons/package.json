{
  "name": "@redesign/material-icons",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "generate": "rm -rf ./src/icons && mkdir ./src/icons && node ./scripts && npm run build",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "emotion": ">=10",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "author": "Otis Virginie",
  "module": "dist/redesign-material-icons.esm.js",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@redesign/theme": "^0.0.1",
    "@redesign/ui-core": "^0.0.1",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-links": "^6.0.10",
    "@storybook/addons": "^6.0.10",
    "@storybook/react": "^6.0.10",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "emotion": "^10.0.27",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "husky": "^4.2.5",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
