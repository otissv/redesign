  "author": "Otis Virginie",
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.36",
    "@types/facepaint": "^1.2.1",
    "@types/jest": "^25.2.3",
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "facepaint": "^1.2.1",
    "rambda": "^5.6.0"
