    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
