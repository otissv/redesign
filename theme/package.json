{
  "name": "@redesign-system/theme",
  "version": "0.0.2",
  "license": "MIT",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "module": "build/redesign-theme.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook -p 6009",
    "dev": "tsdx watch",
