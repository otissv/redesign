{
  "private": true,
  "name": "@redesign-system/ui-core",
  "version": "0.1.0",
  "license": "MIT",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook -p 6007",
    "dev": "tsdx watch",
    "build": "npm run lint && node ../scripts",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "author": "Otis Virginie",
  "peerDependencies": {
    "emotion": ">=10",
    "@redesign-system/theme": ">=0.1",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-a11y": "^6.1.2",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-docs": "^6.1.2",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addons": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@storybook/theming": "^6.1.2",
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.36",
    "@types/facepaint": "^1.2.1",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "emotion": "^10.0.27",
    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "popmotion": "^9.0.0"
